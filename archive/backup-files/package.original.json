{
  "name": "seo-audit-tool",
  "version": "2.0.0",
  "description": "Attention-focused SEO audit tool for agencies and developers",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:ci": "jest --coverage --watchAll=false",
    "lint": "echo 'Linting not configured yet' && exit 0",
    "lint:fix": "echo 'Linting not configured yet' && exit 0",
    "docker:build": "docker build -t seo-audit-tool .",
    "docker:run": "docker run -p 8080:8080 seo-audit-tool",
    "docker:dev": "docker run -p 8080:8080 -v $(pwd):/app seo-audit-tool",
    "deploy:gcp": "gcloud run deploy seo-audit-backend --source . --region=us-central1 --allow-unauthenticated",
    "build:frontend": "mkdir -p dist && cp index.html dist/ && cp example.html dist/ && cp -r assets dist/ 2>/dev/null || true"
  },
  "keywords": [
    "seo",
    "audit",
    "ai",
    "attention",
    "performance",
    "accessibility",
    "schema",
    "puppeteer",
    "web-scraping"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "axe-core": "^4.8.2",
    "cheerio": "^1.0.0-rc.12",
    "chrome-launcher": "^1.1.0",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.3.0",
    "franc": "^6.2.0",
    "keyword-extractor": "^0.0.27",
    "lighthouse": "^12.0.0",
    "natural": "^8.1.0",
    "node-fetch": "^2.7.0",
    "puppeteer": "^24.9.0",
    "sentiment": "^5.0.2",
    "stopword": "^3.1.5",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "jest": "^30.0.5",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4"
  }
}
