{
  "name": "seo-audit-tool-optimized",
  "version": "2.1.0",
  "description": "Optimized lightweight SEO audit tool for fast cloud deployments",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "deploy": "node scripts/deploy.js",
    "build:css": "tailwindcss -i ./index.html -o ./dist/styles.css --minify",
    "build:frontend": "mkdir -p dist && cp index.html dist/ && cp -r components dist/ && npm run build:css"
  },
  "keywords": [
    "seo",
    "audit",
    "performance",
    "lightweight",
    "serverless"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.3.0",
    "node-fetch": "^2.7.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xml2js": "^0.6.2"
  },
  "optionalDependencies": {
    "axe-core": "^4.8.2",
    "chrome-launcher": "^1.1.0",
    "compromise": "^14.14.4",
    "franc": "^6.2.0",
    "lighthouse": "^12.0.0",
    "natural": "^8.1.0",
    "puppeteer-core": "^24.9.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "jest": "^30.0.5",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.17"
  }
}
